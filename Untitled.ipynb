import java.util.*;

class Sorted_Array {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int T = sc.nextInt();
        while (T > 0) {
            int n = sc.nextInt();
            int arr[] = new int[n];
            for (int i = 0; i < n; i++) {
                arr[i] = sc.nextInt();
            }
            GfG obj = new GfG();
            System.out.println(obj.transitionPoint(arr, n));
            T--;
        }
    }
}// } Driver Code Ends


class GfG {
    int transitionPoint(int arr[], int n) {
        int first = 0,last = n-1;
        if(arr[last] == 0) return -1;
        if(arr[0] == 1) return 0;
        int mid = (first+last)/2+1;
        
        while(first<last)
        {
            if(arr[mid] == 1 && arr[mid-1] == 0) 
            {
                //flag = mid;break;
                return mid;
            }
            if(arr[mid] == 0 && arr[mid+1] ==1) return mid+1;
            
            if(arr[mid] == 1) last = mid-1;
            if(arr[mid] ==0) first = mid+1;
            mid = (first+last)/2;
        }
        //if(flag == 0) return -1;
        return 0;
    }
}